{
  "name": "simplia/integration-skeleton",
  "description": "Skeleton project for Simplia API integrations",
  "license": "proprietary",
  "type": "project",
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "simplia/integration": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "post-create-project-cmd": [
      "post_create_project.php"
    ]
  }
}
